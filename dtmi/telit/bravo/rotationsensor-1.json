{
  "@id": "dtmi:telit:bravo:RotationSensor;1",
  "@type": "Interface",
  "displayName": "BravoME910SensorInterfaces",
  "description": "Reports the bravo Device environment Rotation Data information",
  "@context": "dtmi:dtdl:context;2",
  "contents": [
    {
      "@type": [
        "Telemetry",
        "Angle"
      ],
      "displayName": "Rotation X-Axis",
      "name": "rotation_x",
      "schema": "double",
      "unit": "radian",
      "description": "Received Rotation sensor angle in X-Axis (in radian)"
    },
    {
      "@type": [
        "Telemetry",
        "Angle"
      ],
      "displayName": "Rotation Y-Axis",
      "name": "rotation_y",
      "schema": "double",
      "unit": "radian",
      "description": "Received Rotation sensor angle in Y-Axis (in radian)"
    },
    {
      "@type": [
        "Telemetry",
        "Angle"
      ],
      "displayName": "Rotation Z-Axis",
      "name": "rotation_z",
      "schema": "double",
      "unit": "radian",
      "description": "Received Rotation sensor angle in Z-Axis (in radian)"
    },
    {
      "@type": [
        "Telemetry",
        "Angle"
      ],
      "displayName": "Rotation W-Axis",
      "name": "rotation_w",
      "schema": "double",
      "unit": "radian",
      "description": "Received Rotation quaternions (in radian)"
    },
    {
      "@type": [
        "Telemetry",
        "Angle"
      ],
      "displayName": "Rotation estimated accuracy",
      "name": "estimated_accuracy",
      "schema": "double",
      "unit": "radian",
      "description": "Received Rotation estimated accuracy (in radian)"
    }
  ]
}
